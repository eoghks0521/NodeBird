    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends" : [
    "eslint:recommended",
    "plugins : react/recommended"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ]
}
